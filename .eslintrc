{
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "airbnb/hooks",
    "plugin:@typescript-eslint/recommended",
    "plugin:jest/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },

  "plugins": ["react", "react-hooks", "@typescript-eslint", "jest", "prettier"],
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "@typescript-eslint/semi": "off",
    "no-restricted-exports": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/jsx-props-no-spreading": "warn",
    "react/prop-types": "off",
    "react-hooks/exhaustive-deps": "warn",
    "import/prefer-default-export": "off",
    "react/function-component-definition": [0, { "namedComponents": "function-expression" }],
    "func-names": ["warn", "as-needed"]
    // "no-unused-vars": "warn",
    // "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
    // "import/extensions": [
    //   "error",
    //   "ignorePackages",
    //   {
    //     "js": "never",
    //     "jsx": "never",
    //     "ts": "never",
    //     "tsx": "never"
    //   }
    // ]
    // "prettier/prettier": ["error", { "trailingComma": "es5", "singleQuote": true, "printWidth": 120 }]
  }
}
